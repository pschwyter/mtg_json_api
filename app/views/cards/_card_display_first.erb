<table class="table-minimal">
	<tr>
		<th>
			Name
		</th>
		<th>
			Type
		</th>
		<th>
			Cost
		</th>
		<th>
			Set
		</th>
		<th>
			$$$
		</th>
		<th>
			Add to Trade Binder
		</th>
		<th>
			Add to Wishlist
		</th>
	</tr>
	<% @cards.each do |card| %>
		<tr>
			<td id="card-id" data-id=<%= card.id %>>
				<%= card.name %>
			</td>
			<td>
				<%= card.card_types.first.name %>
			</td>
			<td>
				<%= all_mana_cost_images(card) %>
			</td>
			<td>
				<%= card.card_set.name %>
			</td>
			<td>
				$4.95
			</td>

		</tr>
	<% end %>
</table>









