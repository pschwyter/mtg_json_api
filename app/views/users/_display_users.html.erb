<table class="table-minimal">
		<thead>
			<tr>
				<th>Results</th>
			</tr>
			<tr>
				<th>User</th>
				<th>Can Give/ Can Receive</th>
				<th>Last Online</th>
				<th>Distance</th>
				<th>Trade</th>
			</tr>
		</thead>
		<% users.each do |user| %>
			<tr>
				<td>
					<%= link_to user.full_name , user_path(user.id) %>
				</td>
				<td>
					$ 0 / $ 10
				</td>
				<td>
					Online Now!
				</td>
				<td>
					1km
				</td>
				<td>
					<h4><%= link_to "Propose Trade", new_user_trade_path(user.id) %> <br/> </h4>
				</td>
				<br/>
			</tr>	
		<% end %>
</table>