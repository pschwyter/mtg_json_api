<table>
	<tr>
		<th>
			Name
		</th>
		<th>
			Type
		</th>
		<th>
			Cost
		</th>
		<th>
			Set
		</th>
		<th>
			TCGMid
		</th>
		<th>
			Add to Trade Binder
		</th>
		<th>
			Add to Wishlist
		</th>
	</tr>
	<% if @cards %>
		<% @cards.each do |card| %>
			<tr>
				<td>
					<%= card.name %>
				</td>
				<td>
					<%= card.card_types.first.name %>
				</td>
				<td>
					<%= all_mana_cost_images(card) %>
				</td>
				<td>
					<%= card.card_set.name %>
				</td>
				<td>
					<a href="#">$<%= card.price %></a>
				</td>
			</tr>
		<% end %>
	<% end %>

</table>









